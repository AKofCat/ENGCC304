#include <stdio.h>

int main() {
    int num, i, j;

    printf("Enter a number: ");
    scanf("%d", &num);

    i = num;
    do {
        if (i >= 2) {
            j = 2;
            do {
                if (i % j == 0 && i != j)
                    break;
                j++;
            } while (j <= i);

            if (j > i)
                printf("%d ", i);
        }
        i--;
    } while (i >= 2);

    printf("\n");
    return 0;
}