#include <stdio.h>

int main() {
    int NUM;

    printf("Enter a number: ");
    scanf("%d", &NUM);
    int i = NUM;

    do {
        int j = 2;

    do {
        if (i % j == 0 && i != j) 
            break; 
            j++;  

    }while (j <= i);
        if (j > i)
        printf("%d ", i);
        i--;

    }while (i >= 2);
    return 0;
}